<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<LinearLayout
		android:id="@+id/background"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:padding="8dp"
		android:background="#131313"
		android:orientation="vertical">
		<ScrollView
			android:id="@+id/scrollView"
			android:layout_width="match_parent"
			android:layout_height="match_parent">
			<LinearLayout
				android:id="@+id/sv_child"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:orientation="vertical">
				<LinearLayout
					android:id="@+id/o1"
					android:layout_width="match_parent"
					android:layout_height="50dp"
					android:gravity="center_horizontal|center_vertical"
					android:orientation="horizontal">
					<ImageView
						android:id="@+id/imageview19"
						android:focusable="false"
						android:layout_width="40dp"
						android:layout_height="40dp"
						android:padding="4dp"
						android:src="@drawable/web"
						android:scaleType="fitCenter" />
					<LinearLayout
						android:id="@+id/LinearLayout1"
						android:layout_width="match_parent"
						android:layout_height="match_parent"
						android:gravity="center_vertical"
						android:orientation="vertical">
						<TextView
							android:id="@+id/textview7"
							android:focusable="false"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="@string/lang_select"
							android:textSize="14sp"
							android:textColor="#FFFFFF" />
						<TextView
							android:id="@+id/TextView8"
							android:focusable="false"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="@string/settings_curr_language"
							android:textSize="12sp"
							android:textColor="#BDBDBD" />
					</LinearLayout>
				</LinearLayout>
				<LinearLayout
					android:id="@+id/o2"
					android:layout_width="match_parent"
					android:layout_height="50dp"
					android:gravity="center_horizontal|center_vertical"
					android:orientation="horizontal">
					<ImageView
						android:id="@+id/imageview13"
						android:focusable="false"
						android:layout_width="40dp"
						android:layout_height="40dp"
						android:padding="4dp"
						android:src="@drawable/information"
						android:scaleType="fitCenter" />
					<TextView
						android:id="@+id/TextView1"
						android:focusable="false"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="@string/settings_opt_help"
						android:textSize="14sp"
						android:textColor="#FFFFFF"
						android:layout_weight="1" />
				</LinearLayout>
				<LinearLayout
					android:id="@+id/LinearLayout5"
					android:layout_width="match_parent"
					android:layout_height="1dp"
					android:layout_marginLeft="40dp"
					android:padding="8dp"
					android:background="#616161"
					android:orientation="horizontal" />
				<LinearLayout
					android:id="@+id/o3"
					android:layout_width="match_parent"
					android:layout_height="50dp"
					android:gravity="center_horizontal|center_vertical"
					android:orientation="horizontal">
					<ImageView
						android:id="@+id/imageview17"
						android:focusable="false"
						android:layout_width="40dp"
						android:layout_height="40dp"
						android:padding="4dp"
						android:src="@drawable/ic_telegram"
						android:scaleType="fitCenter" />
					<TextView
						android:id="@+id/textview5"
						android:focusable="false"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="@string/settings_link_telegram_group"
						android:textSize="14sp"
						android:textColor="#FFFFFF"
						android:layout_weight="1" />
				</LinearLayout>
				<LinearLayout
					android:id="@+id/o4"
					android:layout_width="match_parent"
					android:layout_height="50dp"
					android:gravity="center_horizontal|center_vertical"
					android:orientation="horizontal">
					<ImageView
						android:id="@+id/imageview18"
						android:focusable="false"
						android:layout_width="40dp"
						android:layout_height="40dp"
						android:padding="4dp"
						android:src="@drawable/ic_telegram"
						android:scaleType="fitCenter" />
					<TextView
						android:id="@+id/textview6"
						android:focusable="false"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="@string/settings_link_telegram_channel"
						android:textSize="14sp"
						android:textColor="#FFFFFF"
						android:layout_weight="1" />
				</LinearLayout>
				<LinearLayout
					android:id="@+id/o5"
					android:layout_width="match_parent"
					android:layout_height="50dp"
					android:gravity="center_horizontal|center_vertical"
					android:orientation="horizontal">
					<ImageView
						android:id="@+id/imageview16"
						android:focusable="false"
						android:layout_width="40dp"
						android:layout_height="40dp"
						android:padding="4dp"
						android:src="@drawable/github"
						android:scaleType="fitCenter" />
					<TextView
						android:id="@+id/textview4"
						android:focusable="false"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="@string/settings_link_github"
						android:textSize="14sp"
						android:textColor="#FFFFFF"
						android:layout_weight="1" />
				</LinearLayout>
			</LinearLayout>
		</ScrollView>
	</LinearLayout>
</LinearLayout>
